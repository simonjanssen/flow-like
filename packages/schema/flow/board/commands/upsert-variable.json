{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "UpsertVariableCommand",
	"type": "object",
	"required": ["variable"],
	"properties": {
		"variable": {
			"$ref": "#/definitions/Variable"
		},
		"old_variable": {
			"anyOf": [
				{
					"$ref": "#/definitions/Variable"
				},
				{
					"type": "null"
				}
			]
		}
	},
	"definitions": {
		"Variable": {
			"type": "object",
			"required": [
				"data_type",
				"editable",
				"exposed",
				"id",
				"name",
				"secret",
				"value_type"
			],
			"properties": {
				"id": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"category": {
					"type": ["string", "null"]
				},
				"description": {
					"type": ["string", "null"]
				},
				"default_value": {
					"type": ["array", "null"],
					"items": {
						"type": "integer",
						"format": "uint8",
						"minimum": 0.0
					}
				},
				"data_type": {
					"$ref": "#/definitions/VariableType"
				},
				"value_type": {
					"$ref": "#/definitions/ValueType"
				},
				"exposed": {
					"type": "boolean"
				},
				"secret": {
					"type": "boolean"
				},
				"editable": {
					"type": "boolean"
				},
				"hash": {
					"type": ["integer", "null"],
					"format": "uint64",
					"minimum": 0.0
				}
			}
		},
		"VariableType": {
			"type": "string",
			"enum": [
				"Execution",
				"String",
				"Integer",
				"Float",
				"Boolean",
				"Date",
				"PathBuf",
				"Generic",
				"Struct",
				"Byte"
			]
		},
		"ValueType": {
			"type": "string",
			"enum": ["Array", "Normal", "HashMap", "HashSet"]
		}
	}
}
